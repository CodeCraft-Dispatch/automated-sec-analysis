    "@nx/module-federation": "20.4.4",
    "@nx/web": "20.4.4",
    "@nx/webpack": "20.4.4",
    "@schematics/angular": "~19.1.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/chalk": "^2.2.4",
