    "@angular/language-service": "~19.1.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@eslint/js": "^9.8.0",
    "@module-federation/enhanced": "^0.8.8",
    "@nx/cypress": "20.4.4",
    "@nx/eslint": "20.4.4",
